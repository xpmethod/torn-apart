    "exports-loader": "^0.7.0",
    "ghooks": "^2.0.4",
    "lodash": "^4.17.10",
    "npm-run-all": "^4.1.3",
    "papaparse": "^4.5.0",
    "rimraf": "^2.6.2",
    "webpack": "^4.15.1",
    "webpack-cli": "^3.0.8"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xpmethod/torn-apart.git"
