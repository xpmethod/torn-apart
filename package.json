{
  "name": "torn-apart",
  "sideEffects": false,
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "npm-run-all --parallel build:*",
    "build:main": "babel -q --copy-files --out-dir dist --ignore *.test.js --plugins syntax-dynamic-import src",
    "build:umd": "webpack -p",
    "lint": "eslint src",
    "prenews": "npm run build:main",
    "news": "node dist/news",
