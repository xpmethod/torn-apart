    "@turf/rhumb-destination": "^6.0.3",
    "csv": "^3.1.0",
    "csv-parse": "^2.5.0",
    "d3": "^5.5.0",
    "d3-cloud": "^1.2.5",
    "d3-simple-slider": "^0.2.1",
    "d3-svg-legend": "^2.25.6",
    "d3-tip": "^0.9.1",
    "eslint-config-prettier": "^3.0.1",
    "jquery": "^3.3.1",
    "leaflet": "^1.3.1",
    "markdown-it": "^8.4.1",
    "markdown-it-footnote": "^3.0.1",
